{
  // Use IntelliSense to learn about possible Node.js debug attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Start Browser Backend",
      "program": "${workspaceRoot}/examples/coffee-example/browser-app/src-gen/backend/main.js",
      "args": ["--loglevel=debug", "--port=3000", "--no-cluster"],
      "env": {
        "NODE_ENV": "development"
      },
      "sourceMaps": true,
      "smartStep": true,
      "internalConsoleOptions": "openOnSessionStart",
      "outputCapture": "std"
    },
    {
      "name": "Launch Browser Frontend",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3000/",
      "sourceMaps": true,
      "webRoot": "${workspaceRoot}/examples/coffee-example/browser-app"
    }
  ]
}
