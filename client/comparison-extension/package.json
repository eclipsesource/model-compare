  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.14.0",
    "@jsonforms/core": "2.4.0",
    "@jsonforms/react": "2.4.0",
    "@jsonforms/vanilla-renderers": "2.4.0",
    "@theia/core": "1.20.0",
    "@theia/process": "1.20.0",
    "@theia/workspace": "1.20.0",
    "@theia/git": "1.20.0",
    "react": "^16.9.13",
    "react-dom": "^16.9.13",
    "react-redux": "^7.1.5",
    "redux": "^4.0.4",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@eclipse-glsp/client": "0.8.0-next.197cba73",
    "@eclipse-glsp/config": "0.9.0-next.a3910f6a",
    "@eclipse-glsp/theia-integration": "0.8.0-next.cef5529d",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "eslint": "^6.8.0",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-react": "^7.20.3",
    "rimraf": "^2.6.1",
    "semver": "^6.3.0",
    "typescript": "^3.9.2"
  },
  "scripts": {
    "all": "yarn run clean && yarn run lint && yarn run build",
    "prepare": "yarn all",
    "clean": "rimraf lib",
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint -c ./.eslintrc.js --ext .ts,.tsx ./src",
    "publish:next": "VERSION_NUMBER=$npm_package_version-next.$(date +%Y%m%d.%H%M%S) && yarn publish --access public --tag next --new-version $VERSION_NUMBER --no-git-tag-version"
  },
  "theiaExtensions": [
    {
      "frontend": "lib/browser/tree-comparison-extension-frontend-module",
      "backend": "lib/node/comparison-extension-backend-module"
    },
    {
      "frontend": "lib/browser/graphical/graphical-comparison-extension-frontend-module"
    }
  ]
}
